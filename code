//code is in cpp

#include <iostream>

using namespace std;

int main()
{
    string a,ans;
    a="harneeeeet";
    int n=a.length();
    for(int i=0;i<n;i++){
        if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u')
        continue;
        ans.push_back(a[i]);
    }
    std::cout << ans << std::endl;
}
